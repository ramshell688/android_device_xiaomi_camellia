# Power
genfscon proc  /proc/cpufreq/cpufreq_cci_mode                                                 u:object_r:proc_cpufreq:s0
genfscon sysfs /sys/module/fbt_cpu/parameters/bhr_opp                                         u:object_r:sysfs_fbt_cpu:s0
genfscon sysfs /sys/kernel/fpsgo/fbt/boost_ta                                                 u:object_r:sysfs_fpsgo:s0

