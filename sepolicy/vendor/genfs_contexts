# Fingerprint
genfscon sysfs /devices/platform/fingerprint                                                  u:object_r:vendor_fingerprint_data_file:s0
genfscon sysfs /devices/platform/fingerprint/clk_enable                                       u:object_r:vendor_fingerprint_data_file:s0
genfscon sysfs /devices/platform/fingerprint/irq                                              u:object_r:vendor_fingerprint_data_file:s0
genfscon sysfs /devices/platform/fingerprint/wakeup_enable                                    u:object_r:vendor_fingerprint_data_file:s0
genfscon sysfs /devices/platform/fingerprint/fingerdown_wait                                  u:object_r:vendor_fingerprint_data_file:s0

# Power
genfscon proc  /cpufreq/cpufreq_cci_mode                                                      u:object_r:proc_cpufreq:s0
genfscon sysfs /module/fbt_cpu/parameters/bhr_opp                                             u:object_r:sysfs_fbt_cpu:s0
genfscon sysfs /kernel/fpsgo/fbt/boost_ta                                                     u:object_r:sysfs_fpsgo:s0
genfscon sysfs /devices/platform/13000000.mali                                                u:object_r:sysfs_gpu:s0
genfscon sysfs /devices/platform/13000000.mali/js_ctx_scheduling_mode                         u:object_r:sysfs_gpu:s0
genfscon sysfs /devices/platform/13000000.mali/dvfs_period                                    u:object_r:sysfs_gpu:s0
genfscon sysfs /devices/platform/13000000.mali/modalias                                       u:object_r:sysfs_gpu:s0

